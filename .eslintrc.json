{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "arrow-spacing": ["warn", { "before": true, "after": true }],
        "no-console": "off",
        "prefer-const": "error",
        "quotes": ["error", "double"]
    }
}
